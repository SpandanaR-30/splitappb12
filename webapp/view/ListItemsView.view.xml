<mvc:View controllerName="app.splitappb12.controller.ListItemsView"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <Page id="page" title="{i18n>title}">

    <headerContent>
			<Button icon="sap-icon://sort" press="onSort"></Button>
			<Button icon="sap-icon://action" tooltip="Share" press="onPressToView2"/>
		</headerContent>
		
		<subHeader>
			<OverflowToolbar>
				<SearchField search="onSearch" id="idSearch" liveChange="onSearch"/>
			</OverflowToolbar>
		</subHeader>
		
		<content>
			
			<List items="{toolModel>/toolsData}" id="idListItem" mode="SingleSelectMaster" selectionChange="onItemPress">
			 <items>
			 	<ObjectListItem icon="{toolModel>icon}" 
			 	intro="{toolModel>id}" number="{toolModel>price}" numberState="{=${toolModel>status}=== 'out of stock' ? 'Error' : 'Success'}"
			 	numberUnit="{toolModel>currency}" title="{toolModel>name}">
			 		<attributes>
			 			<ObjectAttribute title="weight" text="{toolModel>weight}"></ObjectAttribute>
			 		</attributes>
			 		<firstStatus>
			 			<ObjectStatus title="status" text="{toolModel>status}" state="{=${toolModel>status}=== 'out of stock' ? 'Error' : 'Success'}"></ObjectStatus>
			 		</firstStatus>
			 	</ObjectListItem>
			 	
			 	
			 	<!--<DisplayListItem label = "name" value = "mitre saw"> </DisplayListItem>-->
			 	<!--<InputListItem label = "name">-->
			 	<!--<content>-->
			 	<!--	<Label text="description"></Label>-->
			 	<!--	<Input value="good for cutting"></Input>-->
			 	<!--</content>-->
			 	<!--</InputListItem>-->
			 	<!--<StandardListItem icon="data:image/jpeg;base64," -->
			 	<!--description="for smooth cuts" info="600" title="mitre saw"> </StandardListItem>-->
			 	<!--<FeedListItem icon="data:image/jpeg;base64,"-->
			 	<!--info="Rock star" -->
			 	<!--sender="Yash" text="Lets move to the next part of KGF"> </FeedListItem>-->
			 </items>
		</List>
			
		</content>
		

		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button text="Accept" type="Accept" />
				<Button text="Reject" type="Reject" />
				<Button text="Edit" />
				<Button text="Delete" />
			</OverflowToolbar>
		</footer>

    </Page>
</mvc:View>